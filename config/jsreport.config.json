{
  "store": {
    "provider": "fs",
    "fs": {
      "dataDirectory": "data"
  }
  },
  "extensions": {
    "studio": {
      "enabled": true,
      "scripts": true,
      "data": true,
      "assets": true,
      "settings": true,
      "templates": true
    }
  },
  "chrome": {
    "launchOptions": {
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox",
        "--allow-file-access-from-files",
        "--disable-web-security"
      ]
    }
  }
}
